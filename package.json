{
  "name": "@graphiy/layout",
  "version": "0.1.0",
  "description": "Layouts",
  "keywords": [
    "data visualization",
    "layout algorythm"
  ],
  "homepage": "https://github.com/graphiy/layout#readme",
  "license": "GPL-3.0+",
  "main": "dist/layout.js",
  "module": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/Graphiy/layout"
  },
  "scripts": {
    "build": "webpack",
    "lint": "eslint . --ignore-path .gitignore",
    "start": "webpack-dev-server --config demo/webpack.config.babel.js --port 8007",
    "test": "jest test/"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "browser": true
    },
    "extends": "@graphiy/eslint-config",
    "globals": {
      "_": true,
      "$": true
    }
  },
  "dependencies": {
    "d3-force": "^1.0.6",
    "d3-quadtree": "^1.0.3",
    "dat.gui": "^0.7.2",
    "eventemitter3": "^3.1.0",
    "webcola": "^3.3.6"
  },
  "devDependencies": {
    "@graphiy/eslint-config": "^0.1.0",
    "@graphiy/transform": "^0.1.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "d3-dsv": "^1.0.7",
    "d3-ease": "^1.0.3",
    "d3-selection": "^1.1.0",
    "d3-transition": "^1.1.0",
    "eslint": "^4.19.1",
    "jest": "^23.1.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.9.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  },
  "peerDependencies": {
    "lodash": "4.x.x"
  },
  "jest": {
    "testMatch": [
      "**/test/radial.js"
    ],
    "testNamePattern": "^Layout "
  }
}
