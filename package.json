{
  "name": "@graphiy/layout",
  "version": "0.1.0",
  "description": "Layouts",
  "keywords": [
    "data visualization",
    "layout algorythm"
  ],
  "homepage": "https://github.com/graphiy/layout#readme",
  "license": "GPL-3.0+",
  "main": "build/bundle.js",
  "module": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/Graphiy/layout"
  },
  "scripts": {
    "build": "webpack",
    "lint": "eslint . --ignore-path .gitignore",
    "start": "webpack-dev-server --port 8007",
    "test": "jest test/"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "browser": true
    },
    "extends": "@graphiy/eslint-config",
    "globals": {
      "_": true,
      "$": true
    }
  },
  "dependencies": {
    "d3-force": "^1.0.6",
    "d3-quadtree": "^1.0.3",
    "eventemitter3": "^2.0.3",
    "webcola": "^3.3.6"
  },
  "devDependencies": {
    "@graphiy/eslint-config": "graphiy/eslint-config",
    "@graphiy/transform": "graphiy/transform",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "d3-dsv": "^1.0.7",
    "d3-ease": "^1.0.3",
    "d3-selection": "^1.1.0",
    "d3-transition": "^1.1.0",
    "eslint": "^4.6.0",
    "extract-text-webpack-plugin": "2.1.2",
    "jest": "^21.2.1",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.7.1"
  },
  "peerDependencies": {
    "lodash": "4.x"
  },
  "jest": {
    "testMatch": [
      "**/test/radial.js"
    ],
    "testNamePattern": "^Layout "
  }
}
